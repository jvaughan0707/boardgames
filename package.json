{
  "name": "boardgames-server",
  "version": "1.4.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "debug": "~2.6.3",
    "dotenv": "^8.2.0",
    "express": "^4.15.3",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "mongodb": "^3.5.5",
    "mongodb-extjson": "^3.0.3",
    "mongoose": "^8.1.1",
    "morgan": "^1.8.2",
    "socket.io": "^4.7.4"
  },
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "postinstall": "cd client && npm install && npm run build",
    "start": "node server",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^3.0.3"
  },
  "engines": {
    "node": "20.11.0"
